
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  user_id            Int      @id @default(autoincrement())
  email              String   @unique
  has_bought_before  Boolean  @default(false)
  discord_id         String?  @db.VarChar(255)
}